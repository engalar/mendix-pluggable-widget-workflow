  "author": "wengao.liu@siemens.com",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@projectstorm/react-diagrams": "^6.7.0",
    "ahooks": "^3.3.10",
    "classnames": "^2.2.6",
    "dagre": "^0.8.5",
    "lodash-es": "^4.17.21",
    "pathfinding": "^0.4.18",
